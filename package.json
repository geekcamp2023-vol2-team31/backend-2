{
  "type": "commonjs",
  "scripts": {
    "run": "ts-node -r tsconfig-paths/register src/index.ts",
    "dev": "ts-node-dev -r tsconfig-paths/register src/index.ts",
    "build": "tsc -p tsconfig.json",
    "check-types": "npx tsc --noEmit --jsx react",
    "eslint": "eslint src/**/*.ts*",
    "eslint:fix": "eslint src/**/*.ts* --fix",
    "format": "prettier --write \"src/**/*.{tsx,ts,js,json,css,scss}\"",
    "lint": "npm run eslint&&npm run check-types",
    "lint:fix": "npm run format&&npm run eslint:fix&&npm run check-types",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fastify/cookie": "^8.3.0",
    "@fastify/cors": "^8.2.1",
    "@fastify/type-provider-typebox": "^3.0.0",
    "@mgcrea/fastify-session": "^1.1.0",
    "@sinclair/typebox": "^0.27.8",
    "dotenv": "^16.0.3",
    "fastify": "^4.15.0",
    "fastify-socket.io": "^4.0.0",
    "mariadb": "^3.1.1",
    "mysql2": "^3.2.1",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.6.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.14",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.7",
    "types-fastify-socket.io": "^0.0.1",
    "typescript": "^5.0.4"
  }
}
